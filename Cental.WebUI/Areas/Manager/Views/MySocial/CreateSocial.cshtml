@model CreateUserSocialDto
@{
    ViewData["Title"] = "Create Social";
    Layout = "~/Areas/Manager/Views/ManagerLayout/Index.cshtml";
}
<form method="post">
    <label>Social Media Name</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Social Media Icon</label>
    <input type="text" asp-for="Icon" class="form-control" />
    <br />
    <label>Social Media Url</label>
    <input type="text" asp-for="Url" class="form-control" />
    <br />
    <button class="btn btn-outline-dark">Submit</button>

</form>

