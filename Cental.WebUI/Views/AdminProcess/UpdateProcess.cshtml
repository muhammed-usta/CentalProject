@model UpdateProcessDto

@{
    ViewData["Title"] = "UpdateProcess";
    Layout = "~/Views/AdminLayout/Layout.cshtml";
}



<form method="post">
    <input type="hidden" asp-for="ProcessId" />

    <label>Title</label>
    <input type="text" asp-for="Title" class="form-control" />
    <br />
    <label>Description</label>
    <input type="text" asp-for="Description" class="form-control" />
    <br />

    <button class="btn btn-outline-primary"> <i class="fa fa-save"></i> Update</button>
</form>



